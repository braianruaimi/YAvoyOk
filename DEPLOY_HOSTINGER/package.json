{
  "name": "yavoy-app",
  "version": "3.1.0-enterprise",
  "description": "YAvoy v3.1 Enterprise - Sistema de delivery con MySQL",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "enterprise": "node server-enterprise.js",
    "prod": "cross-env NODE_ENV=production node server.js",
    "migrate:mysql": "node migrate-to-mysql.js",
    "test": "jest --detectOpenHandles --forceExit",
    "test:unit": "jest tests/unit --detectOpenHandles --forceExit",
    "test:integration": "jest tests/api --detectOpenHandles --forceExit",
    "test:coverage": "jest --coverage --detectOpenHandles --forceExit",
    "test:watch": "jest --watch",
    "lint": "eslint . --ext .js --ignore-path .gitignore || echo 'ESLint warnings found'",
    "lint:fix": "eslint . --ext .js --fix --ignore-path .gitignore",
    "format": "prettier --write \"**/*.{js,json,md}\" --ignore-path .gitignore",
    "format:check": "prettier --check \"**/*.{js,json,md}\" --ignore-path .gitignore"
  },
  "dependencies": {
    "bcryptjs": "^3.0.3",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "express-rate-limit": "^8.2.1",
    "googleapis": "^171.4.0",
    "helmet": "^8.1.0",
    "joi": "^18.0.2",
    "jsonwebtoken": "^9.0.3",
    "morgan": "^1.10.1",
    "multer": "^2.0.2",
    "mysql2": "^3.16.3",
    "nodemailer": "^8.0.1",
    "qrcode": "^1.5.4",
    "sequelize": "^6.37.7",
    "socket.io": "^4.8.1",
    "speakeasy": "^2.0.0",
    "ua-parser-js": "^2.0.9",
    "web-push": "^3.6.7"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "devDependencies": {
    "cross-env": "^10.1.0",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "prettier": "^3.2.0",
    "supertest": "^6.3.3"
  }
}
